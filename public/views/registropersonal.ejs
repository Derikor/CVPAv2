  <div class="container w-50 mt-5 mb-5rounded-3 shadow fondoima3">
    <div class="row">
      <h2 align="center" class="clores"> PERSONAL</h2>
      <form action="/add" id="personal" method="post">
        <div class="row col-12">
          <div class="section col">
            <label for="name" class="text-white fw-bold">COD PERSONAL:</label>
            <input type="text" name="codp" size="8" placeholder="" required class="border border-dark bg-info"/>
          </div>
          <div class="section col">
            <label for="date" class="text-white fw-bold">HORARIO DE TRABAJO</label>
            <input type="time" name="horadeatencion" required class="border border-dark bg-info"/>
          </div>
          <div class="section col">
            <label for="name" class="text-white fw-bold">CARGO:</label>
            <input class="border border-dark bg-info" type="text" name="especialidad" placeholder="" required />
          </div>
        </div>

        <div class="section">
          <label for="name" class="text-white fw-bold">NOMBRE/S:</label>
          <input type="text" name="nombre" size="28" placeholder="" required class=" w-50 border border-dark bg-info"/>
        </div>
        <div class="section">
          <label for="name" class="text-white fw-bold">APELLIDO PATERNO:</label>
          <input type="text" name="appaterno" placeholder="" required class="w-50 border border-dark bg-info"/>
        </div>
        <div class="section">
          <label for="name" class="text-white fw-bold">APELLIDO MATERNO:</label>
          <input type="text" name="apmaterno" placeholder=""  class="w-50 border border-dark bg-info"/>
        </div>
        <br>
        <div class="section">
          <div align="center"><button class="bg-primary" type="submit" > Agregar Personal </button>
          </div>
        </div>
        <br>
      </form>
    </div>
